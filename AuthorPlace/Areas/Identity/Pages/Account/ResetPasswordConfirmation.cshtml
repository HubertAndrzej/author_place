@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Reset password confirmation";
}

<h1>@ViewData["Title"]</h1>
<p>
    Your password has been reset. Please <a class="link-info link-offset-2 link-underline-opacity-0 link-underline-opacity-25-hover" asp-page="./Login">click here to log in</a>.
</p>
